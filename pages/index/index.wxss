swiper , swiper-item{
  width: 100%;
  height: 298rpx;
}
swiper-item image{
  width: 100%;
  height: 100%;
}
.sort{
    width: 100%;
    height: auto;
}
.sort_w{
    width: 702rpx;
    height: 100%;
    margin: 0 auto;
}
.sort_w .ul{
    width: 100%;
    height: 730rpx;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}
.sort_w .ul .li{
    width: 33.3%;
    height: 33.3%;
    float: left;
}
.sort_w .ul .li navigator{
    height: 243rpx;
    display: flex;
    justify-content: center;
    flex-direction:column;
    align-items:center;
}
 image{
    width: 115rpx;
    height: 115rpx;
    display: block;
}
.sort_w .ul .li text{
    font-size:24rpx;
    color: #8e9397;
    padding-top: 0.33rem;
}
navigator{
  /* -webkit-tap-highlight-color:  rgba(0, 0, 0, 0); */
  outline: none;
}